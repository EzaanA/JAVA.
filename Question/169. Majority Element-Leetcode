public class Majrity_Element {
    public static int finding_majority(int arr[]){
        int count=1;
        int maj_el=arr[0];
        for(int i=1;i<arr.length;i++){
            if(maj_el==arr[i]){
                maj_el=arr[i];
                count++;
            }
            else {
                count--;
                if(count==0){
                    maj_el=arr[i];
                    count++;
                }
            }
        }
        return maj_el;
    }
    public static void main(String[] args) {
        int arr[]={2,3,4,3,3};
        System.out.println(finding_majority(arr));
    }
}
